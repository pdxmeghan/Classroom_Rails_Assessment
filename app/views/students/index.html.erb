<h1>Students</h1>

<ul>
  <% @students.each do |student| %>
    <li><a href="/students/<%= student.id %>"><%= student.name %> </a>||
        <a href="/students/<%= student.id %>/edit"> Edit</a> ||
        <a href="/students/<%= student.id %>"
           data-confirm="Permanently delete student?"
           data-method="delete"
           rel="nofollow">Delete Student</a>
      </li>
  <% end %>
</ul>

<div class="footer">
  <a href="/teachers/new">Add a new teacher</a>
  - <a href="/courses/new">Add a new course</a>
  - <a href="/students/new">Add a new student</a>
  - <a href="/courses">See all courses</a>
  - <a href="/students">See all students</a>
  - <a href="/teachers">Go Home</a>
</div>

